<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WatcherFinal"
        xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml" x:Class="WatcherFinal.MainWindow"
        mc:Ignorable="d"
        Title="Watcher" Height="450" Width="800" ResizeMode="CanMinimize">
    <Grid Background="#FF383838">
        <!--LoginPane-->
        <Grid x:Name="LoginView" Visibility="Hidden" Background="#FF656565">
            <ui:Image Height="125" Width="150" VerticalAlignment="Top" HorizontalAlignment="Center" Source="/src/images/logo.jpg"/>
            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
                <Label Content="User Name :" Foreground="Azure"/>
                <TextBox Background="#FF656565" x:Name="userNameBox" Foreground="Azure"/>
                <Label Content="Password:" Foreground="Azure"/>
                <ui:PasswordBox Background="#FF656565" x:Name="passBox" Foreground="Azure" VerticalAlignment="Center" HorizontalAlignment="Center" MinWidth="150"/>
                <ui:Button Content="Login" Width="150" Click="Login_Click"/>
            </StackPanel>
        </Grid>
        <!--Navigation View-->
        <ui:Image x:Name="LogoMain" Visibility="Hidden" Height="125" Width="150" VerticalAlignment="Top" HorizontalAlignment="Left" Source="/src/images/logo.jpg"/>
        <ui:NavigationView x:Name="NavigationBar" Visibility="Hidden" MinWidth="25" MaxWidth="150" Width="150" Height="290" HorizontalAlignment="Left" VerticalAlignment="Bottom" OpenPaneLength="150" IsBackButtonVisible="Collapsed" IsPaneOpen="False">
            <!--Header-->
            <ui:NavigationView.Header>
                <ui:BreadcrumbBar
                    Margin="42,32,0,0"
                    FontSize="28"
                    FontWeight="DemiBold"
                    />
            </ui:NavigationView.Header>
            <!--Top Menu-->
            <ui:NavigationView.MenuItems>

                <ui:NavigationViewItem Content="Home" Click="NavigationHomeViewItem_Click">
                    <ui:NavigationViewItem.Icon>
                        <ui:SymbolIcon Symbol="Home24" />
                    </ui:NavigationViewItem.Icon>
                </ui:NavigationViewItem>

                <ui:NavigationViewItem Content="Monitor" Click="NavigationMonitorViewItem_Click">
                    <ui:NavigationViewItem.Icon>
                        <ui:SymbolIcon Symbol="ScanCamera48" />
                    </ui:NavigationViewItem.Icon>
                </ui:NavigationViewItem>

                <ui:NavigationViewItem Content="Logs" Click="NavigationLogsViewItem_Click">
                    <ui:NavigationViewItem.Icon>
                        <ui:SymbolIcon Symbol="Book24" />
                    </ui:NavigationViewItem.Icon>
                </ui:NavigationViewItem>

            </ui:NavigationView.MenuItems>
            <!--Bottom Menu-->
            <ui:NavigationView.FooterMenuItems>

                <ui:NavigationViewItem x:Name="isAdminOnline" Visibility="Hidden" Content="Admin Panel" Click="NavigationAdminPanelViewItem_Click">
                    <ui:NavigationViewItem.Icon>
                        <ui:SymbolIcon Symbol="MoreCircle32" />
                    </ui:NavigationViewItem.Icon>
                </ui:NavigationViewItem>

                <ui:NavigationViewItem Content="Settings" Click="NavigationSettingsViewItem_Click">
                    <ui:NavigationViewItem.Icon>
                        <ui:SymbolIcon Symbol="Settings24" />
                    </ui:NavigationViewItem.Icon>
                </ui:NavigationViewItem>

            </ui:NavigationView.FooterMenuItems>
        </ui:NavigationView>
        <!--Home View-->
        <Grid x:Name="HomeView" Visibility="Hidden" Margin="150,0,0,0">
            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" Orientation="Horizontal">
                <ui:Button Content="Serial" Width="100" Height="100" BorderThickness="0" Margin="0,0,10,0" Click="Serial_Click"/>
                <ui:Button Content="Ethernet" Width="100" Height="100" BorderThickness="0" Margin="10,0,0,0" Click="Ethernet_Click"/>
            </StackPanel>
        </Grid>
        <!--Serial Details View-->
        <Grid x:Name="SerialDetailsView" Visibility="Hidden" Margin="150,0,0,0">
            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" Orientation="Horizontal">
                <StackPanel Margin="0,0,15,0">
                    <Label Content="Port:"/>
                    <ComboBox x:Name="portCB" Width="150"/>
                    <Label Content="BaudRate:"/>
                    <ComboBox x:Name="baudCB" Width="150"/>
                    <Label Content="Parity:"/>
                    <ComboBox x:Name="parityCB" Width="150"/>
                </StackPanel>
                <StackPanel>
                    <Label Content="StopBits:"/>
                    <ComboBox x:Name="stopBitsCB" Width="150"/>
                    <Label Content="HandShake:"/>
                    <ComboBox x:Name="handShakeCB" Width="150"/>
                    <Label Content="DataBits:"/>
                    <ComboBox x:Name="dataBitsCB" Width="150"/>
                    <ui:Button Content="Connect" HorizontalAlignment="Right" Click="SerialConnect_Click"/>
                </StackPanel>
                <Button Content="Ethernet" Height="100" Width="100" Margin="15,0,0,0" Click="Ethernet_Click"/>
            </StackPanel>
        </Grid>
        <!--Ethernet Details View-->
        <Grid x:Name="EthernetDetailsView" Visibility="Hidden" Margin="150,0,0,0">
            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" Orientation="Horizontal">
                <Button Content="Serial" Height="100" Width="100" Margin="0,0,15,0" Click="Serial_Click"/>
                <StackPanel Margin="0,0,15,0">
                    <Label Content="Port:"/>
                    <ComboBox x:Name="eportCB" Width="150"/>
                    <Label Content="BaudRate:"/>
                    <ComboBox x:Name="ebaudCB" Width="150"/>
                    <Label Content="Parity:"/>
                    <ComboBox x:Name="eparityCB" Width="150"/>
                </StackPanel>
                <StackPanel>
                    <Label Content="StopBits:"/>
                    <ComboBox x:Name="estopBitsCB" Width="150"/>
                    <Label Content="HandShake:"/>
                    <ComboBox x:Name="ehandShakeCB" Width="150"/>
                    <Label Content="DataBits:"/>
                    <ComboBox x:Name="edataBitsCB" Width="150"/>
                    <ui:Button Content="Connect" HorizontalAlignment="Right" Click="EthernetConnect_Click"/>
                </StackPanel>
            </StackPanel>
        </Grid>
        <!--Monitor View-->
        <Grid x:Name="MonitorView" Visibility="Hidden">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="auto"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Height="150" Width="150">
                <StackPanel>
                    <Label Content="Port : COM9"/>
                    <Label Content="BaudRate : COM9"/>
                    <Label Content="Type : Serial"/>
                    <Label Content="Status : Connected"/>
                    <ui:Button Content="Stop" HorizontalAlignment="Stretch" Click="StopCommunication_Click"/>
                </StackPanel>
            </Grid>
            <Grid Grid.Row="0" Grid.Column="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <TextBlock Text="Card-1" Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="45" Foreground="AliceBlue"/>
                <TextBlock Text="Card-2" Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="45" Foreground="AliceBlue"/>
                <TextBlock Text="Card-3" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="45" Foreground="AliceBlue"/>
            </Grid>
            <Grid Grid.Row="1"
              Grid.Column="1">
                <Grid Grid.Row="1"
              Grid.Column="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <!--Card 1-->
                    <Grid Grid.Column="0" Margin="0,0,15,0" 
                  Visibility="{Binding Card1Visibility[0], UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"
                  >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <!--Card 1 Channel 1-->
                        <Grid Grid.Row="0"
                      Grid.Column="0" Visibility="{Binding Card1Visibility[1], UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" Margin="0,0,1,0"
                      >
                            <Grid.Background>
                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                    <GradientStop Color="Black" Offset="1"/>
                                    <GradientStop Color="#FF5D668C" Offset="0"/>
                                </LinearGradientBrush>
                            </Grid.Background>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="auto"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <!--Open close channel-->
                            <Button x:Name="a0" Grid.Row="0" Grid.Column="0" Width="20" Height="20"
                            Command="{Binding Card1OpenCloseChannel1Command}">
                                <Button.Template>
                                    <ControlTemplate TargetType="{x:Type Button}">
                                        <Grid>
                                            <Ellipse
                                        Fill="{Binding Card1CommunicationStatus[1], UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"
                                        StrokeDashCap="Round" StrokeEndLineCap="Round"
                                        StrokeStartLineCap="Round" StrokeLineJoin="Round" StrokeThickness="4">
                                                <Ellipse.Effect>
                                                    <DropShadowEffect/>
                                                </Ellipse.Effect>
                                                <Ellipse.Stroke>
                                                    <RadialGradientBrush>
                                                        <GradientStop Color="Black" Offset="0"/>
                                                        <GradientStop Color="White" Offset="0.674"/>
                                                        <GradientStop Color="#FF3C3434" Offset="0.798"/>
                                                    </RadialGradientBrush>
                                                </Ellipse.Stroke>
                                            </Ellipse>
                                            <ContentPresenter Content="{TemplateBinding Content}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                        </Grid>
                                    </ControlTemplate>
                                </Button.Template>
                            </Button>

                            <Label Grid.Row="0" Grid.Column="1" Grid.ColumnSpan="4"
                           Content="{Binding ChannelCurrents1[1], UpdateSourceTrigger=PropertyChanged,Mode=OneWay}" FontFamily="Cascadia Mono" 
                           FontSize="10" Foreground="#FF900A00"
                           Opacity="{Binding Card1Activity[1] , UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"/>
                            <!--Voltage Error #FF56FF00 for ok, Red for Error-->
                            <Ellipse Grid.Row="2" Grid.Column="1"
                             Height="20" Width="20" 
                             StrokeThickness="3" 
                             StrokeDashCap="Round" 
                             Stretch="UniformToFill" StrokeEndLineCap="Round" 
                             StrokeStartLineCap="Round"
                             Opacity="{Binding Card1Activity[1] , UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}">
                                <Ellipse.Fill>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="White"/>
                                        <GradientStop Color="{Binding Card1ChannelErrorStatus[2], UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" 
                                              Offset="1"/>
                                    </LinearGradientBrush>
                                </Ellipse.Fill>
                                <Ellipse.Stroke>
                                    <RadialGradientBrush>
                                        <GradientStop Color="#FF050505"/>
                                        <GradientStop Color="#FF545450" Offset="1"/>
                                        <GradientStop Color="#FF8A6914" Offset="0.5"/>
                                    </RadialGradientBrush>
                                </Ellipse.Stroke>
                            </Ellipse>
                            <!--Over Current Error-->
                            <Ellipse Grid.Row="2" Grid.Column="2"
                             Height="20" Width="20" 
                             StrokeThickness="3" StrokeDashCap="Round" 
                             Stretch="UniformToFill" StrokeEndLineCap="Round" 
                             StrokeStartLineCap="Round"
                             Opacity="{Binding Card1Activity[1] , UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}">
                                <Ellipse.Fill>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="White"/>
                                        <GradientStop Color="{Binding Card1ChannelErrorStatus[1], UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" Offset="1"/>
                                    </LinearGradientBrush>
                                </Ellipse.Fill>
                                <Ellipse.Stroke>
                                    <RadialGradientBrush>
                                        <GradientStop Color="#FF050505"/>
                                        <GradientStop Color="#FF545450" Offset="1"/>
                                        <GradientStop Color="#FF8A6914" Offset="0.5"/>
                                    </RadialGradientBrush>
                                </Ellipse.Stroke>
                            </Ellipse>
                            <!--Short Circuit Error-->
                            <Ellipse Grid.Row="2" Grid.Column="3"
                             Height="20" Width="20" StrokeThickness="3" 
                             StrokeDashCap="Round" Stretch="UniformToFill"
                             StrokeEndLineCap="Round" StrokeStartLineCap="Round"
                             Opacity="{Binding Card1Activity[1] , UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}">
                                <Ellipse.Fill>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="White"/>
                                        <GradientStop Color="{Binding Card1ChannelErrorStatus[0], UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" Offset="1"/>
                                    </LinearGradientBrush>
                                </Ellipse.Fill>
                                <Ellipse.Stroke>
                                    <RadialGradientBrush>
                                        <GradientStop Color="#FF050505"/>
                                        <GradientStop Color="#FF545450" Offset="1"/>
                                        <GradientStop Color="#FF8A6914" Offset="0.5"/>
                                    </RadialGradientBrush>
                                </Ellipse.Stroke>
                            </Ellipse>
                        </Grid>
                        <!--Card 1 Channel 2-->
                        <Grid Grid.Row="0" 
                      Grid.Column="1" Visibility="{Binding Card1Visibility[2], UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" Margin="1,0,0,0">
                            <Grid.Background>
                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                    <GradientStop Color="Black" Offset="1"/>
                                    <GradientStop Color="#FF5D668C" Offset="0"/>
                                </LinearGradientBrush>
                            </Grid.Background>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="auto"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <!--Open close channel-->
                            <Button x:Name="a1" Grid.Row="0" Grid.Column="0" Width="20" Height="20"
                            Command="{Binding Card1OpenCloseChannel2Command}">
                                <Button.Template>
                                    <ControlTemplate TargetType="{x:Type Button}">
                                        <Grid>
                                            <Ellipse
                                        Fill="{Binding Card1CommunicationStatus[2], UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"
                                        StrokeDashCap="Round" StrokeEndLineCap="Round"
                                        StrokeStartLineCap="Round" StrokeLineJoin="Round" StrokeThickness="4">
                                                <Ellipse.Effect>
                                                    <DropShadowEffect/>
                                                </Ellipse.Effect>
                                                <Ellipse.Stroke>
                                                    <RadialGradientBrush>
                                                        <GradientStop Color="Black" Offset="0"/>
                                                        <GradientStop Color="White" Offset="0.674"/>
                                                        <GradientStop Color="#FF3C3434" Offset="0.798"/>
                                                    </RadialGradientBrush>
                                                </Ellipse.Stroke>
                                            </Ellipse>
                                            <ContentPresenter Content="{TemplateBinding Content}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                        </Grid>
                                    </ControlTemplate>
                                </Button.Template>
                            </Button>
                            <Label Grid.Row="0" Grid.Column="1" Grid.ColumnSpan="4"
                           Content="{Binding ChannelCurrents1[2], UpdateSourceTrigger=PropertyChanged,Mode=OneWay}" FontFamily="Cascadia Mono"
                           FontSize="10" Foreground="#FF900A00"
                           Opacity="{Binding Card1Activity[2] , UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"/>
                            <!--Voltage Error-->
                            <Ellipse Grid.Row="2" Grid.Column="1"
                             Height="20" Width="20" StrokeThickness="3" 
                             StrokeDashCap="Round" Stretch="UniformToFill" 
                             StrokeEndLineCap="Round" StrokeStartLineCap="Round"
                             Opacity="{Binding Card1Activity[2] , UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}">
                                <Ellipse.Fill>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="White"/>
                                        <GradientStop Color="{Binding Card1ChannelErrorStatus[5], UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" Offset="1"/>
                                    </LinearGradientBrush>
                                </Ellipse.Fill>
                                <Ellipse.Stroke>
                                    <RadialGradientBrush>
                                        <GradientStop Color="#FF050505"/>
                                        <GradientStop Color="#FF545450" Offset="1"/>
                                        <GradientStop Color="#FF8A6914" Offset="0.5"/>
                                    </RadialGradientBrush>
                                </Ellipse.Stroke>
                            </Ellipse>
                            <!--Over Current Error-->
                            <Ellipse Grid.Row="2" Grid.Column="2"
                             Height="20" Width="20" StrokeThickness="3" 
                             StrokeDashCap="Round" Stretch="UniformToFill" 
                             StrokeEndLineCap="Round" StrokeStartLineCap="Round"
                             Opacity="{Binding Card1Activity[2] , UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}">
                                <Ellipse.Fill>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="White"/>
                                        <GradientStop Color="{Binding Card1ChannelErrorStatus[4], UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" Offset="1"/>
                                    </LinearGradientBrush>
                                </Ellipse.Fill>
                                <Ellipse.Stroke>
                                    <RadialGradientBrush>
                                        <GradientStop Color="#FF050505"/>
                                        <GradientStop Color="#FF545450" Offset="1"/>
                                        <GradientStop Color="#FF8A6914" Offset="0.5"/>
                                    </RadialGradientBrush>
                                </Ellipse.Stroke>
                            </Ellipse>
                            <!--Short Circuit Error-->
                            <Ellipse Grid.Row="2" Grid.Column="3"
                             Height="20" Width="20" StrokeThickness="3" 
                             StrokeDashCap="Round" Stretch="UniformToFill" 
                             StrokeEndLineCap="Round" StrokeStartLineCap="Round"
                             Opacity="{Binding Card1Activity[2] , UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}">
                                <Ellipse.Fill>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="White"/>
                                        <GradientStop Color="{Binding Card1ChannelErrorStatus[3], UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" Offset="1"/>
                                    </LinearGradientBrush>
                                </Ellipse.Fill>
                                <Ellipse.Stroke>
                                    <RadialGradientBrush>
                                        <GradientStop Color="#FF050505"/>
                                        <GradientStop Color="#FF545450" Offset="1"/>
                                        <GradientStop Color="#FF8A6914" Offset="0.5"/>
                                    </RadialGradientBrush>
                                </Ellipse.Stroke>
                            </Ellipse>
                        </Grid>
                        <!--Card 1 Channel 3-->
                        <Grid Grid.Row="1" 
                      Grid.Column="0" Visibility="{Binding Card1Visibility[3], UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" Margin="0,0,1,0">
                            <Grid.Background>
                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                    <GradientStop Color="Black" Offset="1"/>
                                    <GradientStop Color="#FF5D668C" Offset="0"/>
                                </LinearGradientBrush>
                            </Grid.Background>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="auto"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <!--Open close channel-->
                            <Button x:Name="a2" Grid.Row="0" Grid.Column="0" Width="20" Height="20"
                            Command="{Binding Card1OpenCloseChannel3Command}">
                                <Button.Template>
                                    <ControlTemplate TargetType="{x:Type Button}">
                                        <Grid>
                                            <Ellipse
                                        Fill="{Binding Card1CommunicationStatus[3], UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"
                                        StrokeDashCap="Round" StrokeEndLineCap="Round"
                                        StrokeStartLineCap="Round" StrokeLineJoin="Round" StrokeThickness="4">
                                                <Ellipse.Effect>
                                                    <DropShadowEffect/>
                                                </Ellipse.Effect>
                                                <Ellipse.Stroke>
                                                    <RadialGradientBrush>
                                                        <GradientStop Color="Black" Offset="0"/>
                                                        <GradientStop Color="White" Offset="0.674"/>
                                                        <GradientStop Color="#FF3C3434" Offset="0.798"/>
                                                    </RadialGradientBrush>
                                                </Ellipse.Stroke>
                                            </Ellipse>
                                            <ContentPresenter Content="{TemplateBinding Content}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                        </Grid>
                                    </ControlTemplate>
                                </Button.Template>
                            </Button>
                            <Label Grid.Row="0" Grid.Column="1" Grid.ColumnSpan="4"
                           Content="{Binding ChannelCurrents1[3], UpdateSourceTrigger=PropertyChanged,Mode=OneWay}" 
                           FontFamily="Cascadia Mono" FontSize="10" 
                           Foreground="#FF900A00"
                           Opacity="{Binding Card1Activity[3] , UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"/>
                            <!--Voltage Error-->
                            <Ellipse Grid.Row="2" Grid.Column="1"
                             Height="20" Width="20" StrokeThickness="3"
                             StrokeDashCap="Round" Stretch="UniformToFill"
                             StrokeEndLineCap="Round" StrokeStartLineCap="Round"
                             Opacity="{Binding Card1Activity[3] , UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}">
                                <Ellipse.Fill>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="White"/>
                                        <GradientStop Color="{Binding Card1ChannelErrorStatus[8], UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" Offset="1"/>
                                    </LinearGradientBrush>
                                </Ellipse.Fill>
                                <Ellipse.Stroke>
                                    <RadialGradientBrush>
                                        <GradientStop Color="#FF050505"/>
                                        <GradientStop Color="#FF545450" Offset="1"/>
                                        <GradientStop Color="#FF8A6914" Offset="0.5"/>
                                    </RadialGradientBrush>
                                </Ellipse.Stroke>
                            </Ellipse>
                            <!--Over Current Error-->
                            <Ellipse Grid.Row="2" Grid.Column="2"
                             Height="20" Width="20" StrokeThickness="3"
                             StrokeDashCap="Round" Stretch="UniformToFill"
                             StrokeEndLineCap="Round" StrokeStartLineCap="Round"
                             Opacity="{Binding Card1Activity[3] , UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}">
                                <Ellipse.Fill>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="White"/>
                                        <GradientStop Color="{Binding Card1ChannelErrorStatus[7], UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" Offset="1"/>
                                    </LinearGradientBrush>
                                </Ellipse.Fill>
                                <Ellipse.Stroke>
                                    <RadialGradientBrush>
                                        <GradientStop Color="#FF050505"/>
                                        <GradientStop Color="#FF545450" Offset="1"/>
                                        <GradientStop Color="#FF8A6914" Offset="0.5"/>
                                    </RadialGradientBrush>
                                </Ellipse.Stroke>
                            </Ellipse>
                            <!--Short Circuit Error-->
                            <Ellipse Grid.Row="2" Grid.Column="3"
                             Height="20" Width="20" StrokeThickness="3"
                             StrokeDashCap="Round" Stretch="UniformToFill"
                             StrokeEndLineCap="Round" StrokeStartLineCap="Round"
                             Opacity="{Binding Card1Activity[3] , UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}">
                                <Ellipse.Fill>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="White"/>
                                        <GradientStop Color="{Binding Card1ChannelErrorStatus[6], UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" Offset="1"/>
                                    </LinearGradientBrush>
                                </Ellipse.Fill>
                                <Ellipse.Stroke>
                                    <RadialGradientBrush>
                                        <GradientStop Color="#FF050505"/>
                                        <GradientStop Color="#FF545450" Offset="1"/>
                                        <GradientStop Color="#FF8A6914" Offset="0.5"/>
                                    </RadialGradientBrush>
                                </Ellipse.Stroke>
                            </Ellipse>
                        </Grid>
                        <!--Card 1 Channel 4-->
                        <Grid Grid.Row="1" 
                      Grid.Column="1"  Visibility="{Binding Card1Visibility[4], UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" Margin="1,0,0,0">
                            <Grid.Background>
                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                    <GradientStop Color="Black" Offset="1"/>
                                    <GradientStop Color="#FF5D668C" Offset="0"/>
                                </LinearGradientBrush>
                            </Grid.Background>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="auto"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <!--Open close channel-->
                            <Button x:Name="a3" Grid.Row="0" Grid.Column="0" Width="20" Height="20"
                            Command="{Binding Card1OpenCloseChannel4Command}">
                                <Button.Template>
